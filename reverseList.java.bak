//T(N) = O()
//S(N) = O()

class Solution {
        
    
        
    public ListNode reverseList(ListNode head) {
        ListNode newHead=null;
        while(head!=null)
        {
            ListNode temp=head.next;
            head.next=newHead;
            newHead=head;
            head=temp;
            
        }
        return newHead;
            
        
    }
}